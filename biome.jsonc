{
	"$schema": "https://biomejs.dev/schemas/1.6.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noConsoleLog": "error"
			}
		}
	},
	"formatter": {},
	"javascript": {
		"formatter": {
			"jsxQuoteStyle": "single",
			"quoteStyle": "single"
		}
	},
	"vcs": {
		"defaultBranch": "main",
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	}
}
